{
    "env": {
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "ignorePatterns": ["lib/**/lib/**/*.js"],
    "rules": {
        "new-cap":"off",
        "prettier/prettier": "error",
        "require-jsdoc": "off",
        "valid-jsdoc": "off"
    }
}
